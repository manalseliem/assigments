#include <iostream>
using namespace std;

int main()
{

char a = '~';
char b = '&';
char c = '%';
char d = 'A';

cout << "ASCII Value of " << a << " Is  " << int('~') << endl;
cout << "ASCII Value of " << b << " Is  " << int('&') << endl;
cout << "ASCII value of " << c << " Is  " << int('%') << "\n";
cout << "ASCII value of " << d << " Is  " << int('A') << endl;



return 0;
}
// Output Needed
/*"ASCII Value of ~ Is 126"
"ASCII Value of & Is 38"
"ASCII Value of % Is 37"
"ASCII Value of A Is 65"*/
